{
    "manifest_version": 3,
    "name": "Video URL Extractor",
    "version": "1.0",
    "description": "Copy the URL of selected image or video on a website.",
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  